<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.yangzhe.learnthreadpool.TestThreadPoolActivity">

    <Button
        android:id="@+id/btn_fixed_thread_pool"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FixedThreadPool" />

    <Button
        android:id="@+id/btn_cached_thread_pool"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CachedThreadPool" />

    <Button
        android:id="@+id/btn_single_thread_pool"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SingleThreadPool" />

    <Button
        android:id="@+id/btn_scheduled_thread_pool"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ScheduleThreadPool" />

    <Button
        android:id="@+id/btn_shutdown_scheduledpool"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ScheduleShutdown" />

</LinearLayout>
